[
    crate core {
        trait CoreTrait<> {}

        trait Unit<> {
            type Assoc<> : [];
        }

        impl<ty T> Unit<> for T {
            type Assoc<> = ();
        }
    },
    crate foo {
        struct FooStruct<> {}
        impl<> CoreTrait<> for <FooStruct<> as Unit<>>::Assoc<> {}
    }
]
