// Test that we detect "indirect" overlap -- here `Foo` is implemented for `u32`
// and also all `T: Is`, and `u32: Is`.
[
    crate core {
        trait Foo<> {}
        impl<> Foo<> for u32 {}
        impl<ty T> Foo<> for T where T: Is<> {}

        trait Is<> {}
        impl<> Is<> for u32 {}
    }
]
